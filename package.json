{
  "name": "m-learning-nest-api",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "author": "",
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "node dist/main.js",
    "start:dev": "cross-env NODE_ENV=test nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "cross-env NODE_ENV=test nest start",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "cross-env NODE_ENV=test jest --config ./test/jest-e2e.json --verbose --detectOpenHandles --forceExit",
    "test:watch": "jest --watch",
    "test:cov": "cross-env NODE_ENV=test jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "seed": "node dist/database/seeder",
    "seed:refresh": "node dist/database/seeder --refresh",
    "clear_jest": "jest --clearCache",
    "eslint-check": "eslint --no-error-on-unmatched-pattern src/**/*.ts"
  },
  "pre-commit": [
    "eslint-check"
  ],
  "dependencies": {
    "@anchan828/nest-sendgrid": "^0.2.1",
    "@casl/ability": "^6.5.0",
    "@nestjs-modules/mailer": "^1.7.1",
    "@nestjs/axios": "^3.0.1",
    "@nestjs/common": "^10.2.10",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.2.10",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/mongoose": "^10.0.2",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.2.10",
    "@nestjs/platform-socket.io": "^10.3.0",
    "@nestjs/platform-ws": "^10.2.10",
    "@nestjs/serve-static": "^4.0.0",
    "@nestjs/swagger": "^7.1.16",
    "@nestjs/websockets": "^10.3.0",
    "@sendgrid/mail": "^8.1.0",
    "aws-sdk": "^2.1518.0",
    "axios": "^1.6.2",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "class-validator-jsonschema": "^5.0.0",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "dotenv": "^16.3.1",
    "express-ctx": "^0.1.1",
    "express-rate-limit": "^7.1.5",
    "fs": "^0.0.1-security",
    "handlebars": "^4.7.8",
    "hbs": "^4.2.0",
    "helmet": "^7.1.0",
    "messagebird": "^4.0.1",
    "mongoose": "^8.0.3",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nestjs-seeder": "^0.3.2",
    "nodemailer": "^6.9.7",
    "nodemon": "^3.0.2",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "password-hash": "^1.2.2",
    "path": "^0.12.7",
    "reflect-metadata": "^0.1.14",
    "request-ip": "^3.3.0",
    "rimraf": "^5.0.5",
    "rxjs": "^7.8.1",
    "swagger-ui-express": "^5.0.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.2.1",
    "@nestjs/schematics": "^10.0.3",
    "@nestjs/testing": "^10.2.10",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.4",
    "@types/passport-jwt": "^3.0.13",
    "@types/supertest": "^2.0.16",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.33.2",
    "jest": "^29.7.0",
    "pre-commit": "^1.2.2",
    "prettier": "^3.1.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3",
    "typescript-eslint-parser": "^18.0.0"
  },
  "jest": {
    "rootDir": "src",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageReporters": [
      "json"
    ],
    "coverageDirectory": "../coverage"
  }
}
